pipeline {
  agent any  
    tools {
        maven 'm7'
    }
    stages {
        stage("Scan") {
            steps {
                script {
                     withSonarQubeEnv(installationName: 'sonarqube') {
                         sh 'mvn clean org.sonar.scanner.maven:sonar-maven-plugin:6.1.0.4477:sonar'        
                    }
                }
            }
        }
    }
    
}
